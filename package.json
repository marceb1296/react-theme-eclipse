{
  "name": "react-theme-eclipse",
  "version": "0.0.0",
  "type": "module",
  "description": "Implement in your React App a Responsive Theme Component easily",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "eslintConfig": {
		"extends": [
			"./node_modules/standard/eslintrc.json"
		]
	},
	"license": "MIT",
	"files": [
		"dist"
	],
	"author": {
		"email": "marce1996hr@gmail.com",
		"name": "Marcelino Huerta"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/marceb1296/react-theme-eclipse.git"
	},
	"bugs": {
		"url": "https://github.com/marceb1296/react-theme-eclipse.git/issues"
	},
	"main": "./dist/react-theme-eclipse.umd.js",
	"module": "./dist/react-theme-eclipse.js",
	"types": "./dist/react-theme-eclipse.d.ts",
	"exports": {
		".": {
			"import": "./dist/react-theme-eclipse.js",
			"require": "./dist/react-theme-eclipse.umd.js"
		},
		"./style": {
			"import": "./dist/style.css",
			"require": "./dist/style.css"
		},
		"./types": {
			"import": "./dist/types/index.d.ts",
			"require": "./dist/types/index.d.ts"
		}
	}
}
